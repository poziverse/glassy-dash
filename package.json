{
  "name": "glassy-dash",
  "private": true,
  "version": "1.1.6",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n WEB,API,SCHEDULER -c auto \"vite\" \"npm:api\" \"npm:scheduler\"",
    "api": "cross-env NODE_ENV=development nodemon server/index.js",
    "scheduler": "node server/scheduler.js start",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit && npm run test:api",
    "test:unit": "vitest run",
    "test:api": "vitest run -c vitest.api.config.js",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint . --ext js,jsx,ts,tsx --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "migrate": "node server/migrations/index.js up",
    "migrate:rollback": "node server/migrations/index.js down",
    "migrate:status": "node server/migrations/index.js status",
    "db:backup": "node server/migrations/backup.js create",
    "db:restore": "node server/migrations/backup.js restore",
    "db:list": "node server/migrations/backup.js list",
    "db:cleanup": "node server/migrations/backup.js cleanup",
    "db:stats": "node server/migrations/backup.js stats",
    "trash:cleanup": "node server/cleanup.js run",
    "trash:stats": "node server/cleanup.js stats",
    "scheduler:start": "node server/scheduler.js start",
    "scheduler:stop": "node server/scheduler.js stop",
    "scheduler:trigger": "node server/scheduler.js trigger",
    "scheduler:status": "node server/scheduler.js status"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@google/generative-ai": "^0.24.1",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-virtual": "^3.13.18",
    "@tiptap/extension-bubble-menu": "^3.17.1",
    "@tiptap/extension-placeholder": "^3.17.1",
    "@tiptap/extension-underline": "^3.17.1",
    "@tiptap/react": "^3.17.1",
    "@tiptap/starter-kit": "^3.17.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dompurify": "^3.3.1",
    "emoji-picker-react": "^4.17.2",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "framer-motion": "^12.23.12",
    "fuse.js": "^7.1.0",
    "helmet": "^8.1.0",
    "idb-keyval": "^6.2.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.539.0",
    "marked": "^16.1.2",
    "node-cron": "^4.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.1.0",
    "react-grid-layout": "^2.2.2",
    "react-hot-toast": "^2.6.0",
    "sqlite3": "^5.1.7",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^4.0.17",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.8.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.3",
    "vite": "^7.1.1",
    "vitest": "^4.0.17"
  }
}
